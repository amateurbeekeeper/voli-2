{
  "name": "voli-2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "node scripts/dev-with-log.mjs",
    "dev:app": "nx run core-web-app:dev",
    "build": "node scripts/build-with-log.mjs",
    "build:raw": "nx run core-web-app:build",
    "lint": "node scripts/lint-with-log.mjs",
    "preview": "nx run core-web-app:preview",
    "storybook": "nx run design-system:storybook",
    "build-storybook": "nx run design-system:build-storybook",
    "test": "node scripts/unit-test-with-log.mjs",
    "test:watch": "nx run-many -t test --parallel",
    "test:e2e": "node scripts/e2e-with-log.mjs",
    "test:e2e:ui": "playwright test --ui",
    "suite": "npm run lint && npm run test && npm run build",
    "suite:e2e": "npm run suite && npm run test:e2e",
    "docker:verify": "docker build --target verify -t voli-2-verify .",
    "docker:web": "docker build -f docker/web.Dockerfile --target serve -t voli-2-web .",
    "docker:storybook": "docker build -f docker/storybook.Dockerfile --target serve -t voli-2-storybook .",
    "docker:up": "docker compose -f docker/docker-compose.yml up --build",
    "ci:fetch-logs": "node scripts/fetch-ci-logs.mjs",
    "render:fetch-logs": "node scripts/render-fetch-logs.mjs",
    "render:fetch-logs:runtime": "node scripts/render-fetch-logs.mjs --runtime",
    "prepare": "husky"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.11.2",
    "@supabase/supabase-js": "^2.95.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "styled-components": "^6.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@chromatic-com/storybook": "^5.0.0",
    "@commitlint/cli": "^20.4.1",
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^9.39.1",
    "@nx/devkit": "22.4.5",
    "@nx/js": "^22.4.5",
    "@nx/playwright": "22.4.5",
    "@nx/react": "^22.4.5",
    "@nx/storybook": "^22.4.5",
    "@nx/vite": "^22.4.5",
    "@nx/web": "22.4.5",
    "@playwright/test": "^1.58.2",
    "@storybook/addon-a11y": "^10.2.7",
    "@storybook/addon-docs": "^10.2.7",
    "@storybook/addon-onboarding": "^10.2.7",
    "@storybook/addon-vitest": "^10.2.7",
    "@storybook/react-vite": "^10.2.7",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/styled-components": "^5.1.36",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.0",
    "ajv": "^8.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jiti": "2.4.2",
    "jsdom": "^28.0.0",
    "nx": "^22.4.5",
    "playwright": "^1.58.2",
    "prettier": "~3.6.2",
    "storybook": "^10.2.7",
    "ts-node": "10.9.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vite-plugin-dts": "~4.5.0",
    "vitest": "^4.0.18"
  }
}
